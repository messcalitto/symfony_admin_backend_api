<!DOCTYPE html>
<html>
    <head>
        <meta charset="UTF-8">
        <title>Users</title>
    </head>
    <body>
        <h1>Users</h1>
        <a href="{{ path('user_new') }}">Add new user</a>
        <ul>
            {% for user in users %}
                <li>
                    <a href="{{ path('user_show', {id: user.id}) }}">{{ user.name }}</a>
                </li>
            {% endfor %}
        </ul>
    </body>
</html>
